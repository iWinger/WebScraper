  <script>
   function next(){
    h = window.location.href
    a = h.split('/')
    if (a[4]==undefined)
        {window.location.href=h+"/2"}
     else
     {
     var x = Number(a[4])
     if(x==10){
     window.location.href=h.substring(0,28)+"/10"
     }
     else{
     x = x+1
     <!--alert(h.substring(0,28))-->
     window.location.href=h.substring(0,28)+"/"+x.toString()}
     }
    }
    function prev(){
    h = window.location.href
    a = h.split('/')
    if (a[4]==undefined)
        {window.location.href=h}
     else
     {
     if(a[4]=='1'){
     window.location.href=h.substring(0,28)+"/1"
     }
     else{
     var x = Number(a[4])
     x = x-1
     <!--alert(h.substring(0,28))-->
     window.location.href=h.substring(0,28)+"/"+x.toString()}
     }
    }
  </script>
